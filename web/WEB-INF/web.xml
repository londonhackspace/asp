<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2.3.dtd">

<web-app>
        <listener>
            <listener-class>org.aspcloud.engine.asp.AspSessionHandler</listener-class>
        </listener>
        <servlet>
                <servlet-name>
                        asp
                </servlet-name>
                <servlet-class>
                        org.aspcloud.engine.asp.AspServlet
                </servlet-class>
        </servlet>
        <servlet-mapping>
                  <servlet-name>
                          asp
                  </servlet-name>
                  <url-pattern>
                          *.asp
                </url-pattern>
        </servlet-mapping>
        <welcome-file-list>
                <welcome-file>
                        default.asp
                </welcome-file>
                <welcome-file>
                        Default.asp
                </welcome-file>
                <welcome-file>
                        index.asp
                </welcome-file>
        </welcome-file-list>
</web-app>

